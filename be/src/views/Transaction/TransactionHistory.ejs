                                <th>Date</th>
                                <th>Amount</th>
                                <th>Status</th>
                            </tr>
                        </thead>
                        <tbody>

                            <script>

                                // Mảng chứa dữ liệu của các đơn hàng
                                const orders = [
                                    { id: '#153587', date: 'Jan 26, 2023', amount: '18.000.000', status: 'Success' },
                                    { id: '#12436', date: 'Feb 12, 2033', amount: '10.000.000', status: 'Success' },
                                    { id: '#654321', date: 'Mar 15, 2023', amount: '25.000.000', status: 'Pending' },
