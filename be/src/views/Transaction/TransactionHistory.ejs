                                    { id: '#369258', date: 'Aug 17, 2023', amount: '22.000.000', status: 'Pending' },
                                    { id: '#456123', date: 'Sep 28, 2023', amount: '28.000.000', status: 'Success' },
                                    { id: '#951753', date: 'Oct 5, 2023', amount: '19.000.000', status: 'Pending' }
                                ];

                                for (let i = 0; i < orders.length; i++) {
                                    const order = orders[i];
                                    document.write(`<tr>
                                        <td>${order.id}</td>
                                        <td>${order.date}</td>
                                        <td>${order.amount}</td>
                                        <td>${order.status}</td>
                                    </tr>`);
                                }
                            </script>
